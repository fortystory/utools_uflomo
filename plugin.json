{
    "logo": "icons/logo.png",
    "preload": "preload.js",
    "features": [{
        "code": "uflomo_add_width_tag",
        "explain": "uflomo: 新的memo",
        "icon": "icons/logo.png",
        "cmds": [
            "memo",
            "uflomo:memo",
            {
                "type": "regex",
                "label": "uflomo:memo",
                "match": "/^#\\S+/",
                "minLength": 2
            }
        ]
    }, {
        "code": "uflomo_custom_tag",
        "explain": "uflomo: 管理本地标签",
        "icon": "icons/logo.png",
        "cmds": [
            "uflomo:管理标签",
            "uflomo:grlibnqm",
            "uflomo:custom_tag"
        ]
    }, {
        "code": "uflomo_open_local_dir",
        "explain": "uflomo: 打开本地文件目录",
        "icon": "icons/logo.png",
        "cmds": [
            "uflomo:打开本地文件目录",
            "uflomo:dakdbfdiwfjmmulu",
            "uflomo:open_local_dir"
        ]
    }, {
        "code": "uflomo_config_api",
        "explain": "uflomo: 配置api",
        "icon": "icons/logo.png",
        "cmds": [
            "uflomo:配置api",
            {
                "type": "regex",
                "label": "uflomo:配置api",
                "match": "/^https:\\/\\/flomoapp\\.com\\/[\\w/]+$/",
                "minLength": 60
            }
        ]
    }],
    "pluginSetting": {
        "single": false
    }
}