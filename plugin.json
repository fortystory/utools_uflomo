{
    "logo": "icons/logo.png",
    "platform": ["win32"],
    "preload": "preload.js",
    "features": [{
        "code": "uflomo_add_width_tag",
        "explain": "uflomo: 新的memo",
        "icon": "icons/logo.png",
        "platform": ["win32"],
        "cmds": [
            "memo",
            "uflomo:memo",
            {
                "type": "regex",
                "label": "uflomo:memo",
                "match": "/^#\\S+/",
                "minLength": 2
            }
        ]
    }, {
        "code": "uflomo_custom_local_tag",
        "explain": "uflomo: 管理本地标签",
        "icon": "icons/logo.png",
        "platform": ["win32"],
        "cmds": [
            "uflomo:管理本地标签",
            "uflomo:grlibfdibnqm",
            "uflomo:custom_local_tag"
        ]
    }, {
        "code": "uflomo_sort_local_tag",
        "explain": "uflomo: 排序本地标签",
        "icon": "icons/logo.png",
        "platform": ["win32"],
        "cmds": [
            "uflomo:排序本地标签",
            "uflomo:bfdibnqmpdxu",
            "uflomo:sort_local_tag"
        ]
    }, {
        "code": "uflomo_config_api",
        "explain": "uflomo: 配置api",
        "icon": "icons/logo.png",
        "platform": ["win32"],
        "cmds": [
            "uflomo:配置api",
            {
                "type": "regex",
                "label": "uflomo:配置api",
                "match": "/^https:\\/\\/flomoapp\\.com\\/[\\w/]+$/",
                "minLength": 60
            }
        ]
    }],
    "pluginSetting": {
        "single": false
    }
}